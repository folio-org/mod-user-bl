{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Reset password action entity",
  "type": "object",
  "properties": {
    "id": {
      "description": "ID of the password reset action",
      "type": "string"
    },
    "userId": {
      "description": "User ID to register password reset action",
      "type": "string"
    },
    "expirationTime": {
      "description": "Action expiration time",
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "userId",
    "expirationTime"
  ]
}
