{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Manual blocks",
  "properties": {
    "manualblocks": {
      "type": "array",
      "description": "The manual blocks of a user",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Block id"
          },
          "userId": {
            "type": "string",
            "description": "User id"
          }
        },
        "additionalProperties": true
      }
    },
    "totalRecords": {
      "type": "number",
      "description": "Number of manual blocks"
    }
  },
  "additionalProperties": false
}
